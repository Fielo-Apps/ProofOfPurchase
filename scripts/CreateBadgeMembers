/*
    Create BadgeMembers
*/
List<FieloEE__Member__c> members = [Select id, name from FieloEE__Member__c limit 10];
List<FieloEE__Badge__c> badges = [Select id, name from FieloEE__Badge__c];

List<FieloEE__BadgeMember__c> badgeMember = new List<FieloEE__BadgeMember__c>();
for(FieloEE__Member__c m : members){
    badgeMember.add( new FieloEE__BadgeMember__c(Name = badges.get(1).Name,FieloEE__Badge2__c = badges.get(1).Id, FieloEE__Member2__c = m.Id) );
}

insert badgeMember;
