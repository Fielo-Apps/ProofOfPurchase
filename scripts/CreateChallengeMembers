/*
    Create ChallengeMembers
*/
List<FieloEE__Member__c> members = [Select id, name from FieloEE__Member__c];
id challengeId = [select id from fieloch__Challenge__c limit 1].Id;
List<FieloCH__ChallengeMember__c> challengeMember = new List<FieloCH__ChallengeMember__c>();
for(FieloEE__Member__c m : members){
    challengeMember.add( new FieloCH__ChallengeMember__c(Challenge2__c = challengeId, Member__c = m.Id) );
}
insert challengeMember;